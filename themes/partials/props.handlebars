<div id="property_{{name}}" class="property item{{#if access}} {{access}}{{/if}}{{#if deprecated}} deprecated{{/if}}{{#if extended_from}} inherited{{/if}}">
    <h3 class="name"><code>{{name}}</code></h3>
    <span class="type">{{#crossLink type}}{{/crossLink}}</span>

    {{#if deprecated}}
        <span class="flag deprecated"{{#if deprecationMessage}} title="{{deprecationMessage}}"{{/if}}>depreciado</span>
    {{/if}}

    {{#if access}}
        <span class="flag {{access}}">{{access}}</span>
    {{/if}}

    {{#if final}}
        <span class="flag final">final</span>
    {{/if}}

    {{#if static}}
        <span class="flag static">estático</span>
    {{/if}}

    <div class="meta">
        {{#if overwritten_from}}
            <p>Herdado de
            <a href="{{crossLinkRaw overwritten_from/class}}#property_{{overwritten_from/name}}">
                {{overwritten_from/class}}
            </a>
            {{#if foundAt}}
            mas sobrescrito em
            {{/if}}
        {{else}}
            {{#if extended_from}}
                <p>Herdado em
                <a href="{{crossLinkRaw extended_from}}#property_{{name}}">{{extended_from}}</a>:
            {{else}}
                {{#providedBy}}
                    <p>Provido pelo módulo <a href="../modules/{{.}}.html">{{.}}</a>.</p>
                {{/providedBy}}
                <p>
                {{#if foundAt}}
                Definido em
                {{/if}}
            {{/if}}
        {{/if}}
        {{#if foundAt}}
        <a href="{{foundAt}}">`{{{file}}}:{{{line}}}`</a>
        {{/if}}
        </p>

        {{#if deprecationMessage}}
            <p>Depreciado: {{deprecationMessage}}</p>
        {{/if}}

        {{#if since}}
            <p>Disponível desde {{since}}</p>
        {{/if}}
    </div>

    <div class="description">
        {{{propertyDescription}}}
    </div>

    {{#if default}}
        <p><strong>Padrão:</strong> {{default}}</p>
    {{/if}}

    {{#example}}
        <div class="example">
            <h4>Exemplo:</h4>

            <div class="example-content">
                {{{.}}}
            </div>
        </div>
    {{/example}}

    {{#if subprops}}
        <h4>Sub-propriedades:</h4>

        <ul class="params-list">
            {{#subprops}}
            <li class="param">
                {{#if optional}}
                    <code class="param-name optional">[{{name}}{{#if optdefault}}={{optdefault}}{{/if}}]</code>
                    <span class="type">{{#crossLink type}}{{/crossLink}}</span>
                    <span class="flag optional" title="Esta propriedade é opcional.">opcional</span>
                {{else}}
                    <code class="param-name">{{name}}</code>
                    <span class="type">{{#crossLink type}}{{/crossLink}}</span>
                {{/if}}

                <div class="param-description">
                    {{{description}}}
                </div>

                {{#if subprops}}
                    <ul class="params-list">
                        {{#subprops}}
                        <li class="param">
                            {{#if optional}}
                                <code class="param-name optional">[{{name}}{{#if optdefault}}={{optdefault}}{{/if}}]</code>
                                <span class="type">{{#crossLink type}}{{/crossLink}}</span>
                                <span class="flag optional" title="Esta propriedade é opcional.">opcional</span>
                            {{else}}
                                <code class="param-name">{{name}}</code>
                                <span class="type">{{#crossLink type}}{{/crossLink}}</span>
                            {{/if}}

                            <div class="param-description">
                                {{{description}}}
                            </div>
                        </li>
                        {{/subprops}}
                    </ul>
                {{/if}}
            </li>
            {{/subprops}}
        </ul>
    {{/if}}
</div>
